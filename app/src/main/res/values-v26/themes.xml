<?xml version="1.0" encoding="utf-8"?>
<!-- res/values-v26/themes.xml -->
<resources>

    <style name="AppTheme.SplashScreen" parent="Theme.SplashScreen">
        <!--배경 색-->
        <item name="windowSplashScreenBackground">@color/green_400</item>
        <!--아이콘-->
        <item name="windowSplashScreenAnimatedIcon">@drawable/ic_seulseul</item>
        <!--Animation이 그려지는 Max 시간. 해당 시간이 지나면 애니메이션이 동작하는 도중이라도 마지막 결과로 이동한다.-->
        <item name="windowSplashScreenAnimationDuration">1000</item>

        <item name="postSplashScreenTheme">@style/Theme.SeulSeul</item>
    </style>

</resources>